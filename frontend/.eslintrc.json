{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
  "spellcheck"

  ],
  "rules": {
    "no-extra-semi": 0,
    "spellcheck/spell-checker": ["warn", {
      "skipWords": [
        "req",
        "morgan",
        "minlength"
      ],
      "skipIfMatch": [
        "eslint-*",
        "(= require)*"
    ]
    }]
  }
}
